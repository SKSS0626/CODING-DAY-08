#include<stdio.h>
int main()
{
	int a,b,c;
	printf("Enter three numbers:");
	scanf("%d %d %d",&a,&b,&c);
	if(a<b)
	{
		if(c<b)
		{
			printf("%d is biggest number",b);
		}
		else
		{
			printf("%d is the biggest number",c);
		}
	}
	else
	{
		if(a<c)
		{
			printf("%d is the biggest number",c);
		}
		else
		{
			printf("%d is the biggest number",a);
		}
	}
	return 0;
}
